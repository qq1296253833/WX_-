.search-bar{
  padding:20rpx;
  background-color: #41be57;
}
.search-navigator{
  width:100%;
  height:60rpx;
  background-color: #fff;
  border-radius:10rpx;
  background-image:url("data:image/png;base64,");

  background-position: center center;
  background-size:7%;
  background-repeat:no-repeat;
}
.search-input-group{
  background-color: #fff;
  padding:10rpx 20rpx;
  border-radius: 10rpx;
  border-size:border-box;
}
.search-input-group .search-input{
  width:100%;
  min-height: 40rpx;
  height:40rpx;
  background-color:#fff;
}